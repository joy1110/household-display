{
  "extends": ["next/core-web-vitals", "airbnb", "airbnb/hooks", "airbnb-typescript"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}